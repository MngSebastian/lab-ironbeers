<div>
    {{#each randomBeer}}
    <div>
        <img class="randombeer-img" src='{{this.image_url}}' alt='' />
        <p>{{this.name}}</p>
        <p>{{this.description}}</p>
        <p>{{this.tagline}}</p>
        <h2>Food Pairing</h2>
        <ul>
            {{#each this.food_pairing}}
            <li>{{this}}</li>
            {{/each}}
        </ul>


        <p><b>Brewers Tips: </b><span>{{this.brewers_tips}}</span></p>

        {{!-- <p>{{this.brewers_tips}}</p> --}}
    </div>

    {{/each}}
</div>